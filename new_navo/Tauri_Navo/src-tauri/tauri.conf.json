{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "test2",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "label": "home",
        "fullscreen": false,
        "height": 702,
        "resizable": false,
        "title": "Navo",
        "width": 1250,
        "decorations": false,
        "url": "index.html",
        "visible": true,
        "center": true 
      },
      {
        "label": "list",
        "fullscreen": false,
        "height": 650,
        "resizable": false,
        "title": "Navo",
        "width": 875,
        "decorations": false,
        "url": "List/index.html",
        "visible": false,
        "center": true
      },
      {
        "label": "account",
        "fullscreen": false,
        "height": 650,
        "resizable": false,
        "title": "Navo",
        "width": 875,
        "decorations": false,
        "url": "Account/index.html",
        "visible": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
